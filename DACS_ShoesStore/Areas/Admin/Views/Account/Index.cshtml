@model IEnumerable<DACS_ShoesStore.Models.ApplicationUser>

@{
    ViewBag.Title = "Quan Ly Tai Khoang";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}




    <section class="content">
        <div class="card">
            <div class="card-header">
                <h3 class="card-title">Danh Sách Tài Khoảng</h3>

                <div class="card-tools">
                    <a href="~/Admin/Account/Create" class="btn btn-success"> Them Moi</a>
                </div>
            </div>
            <div class="card-body">
                <table class="table table-bordered">
                    <thead>
                    <td>STT</td>
                    <td>Tai Khoảng</td>
                    <td>Số Điện Thoại</td>
                    <td>Email</td>
                    <td></td>
                    </thead>
                    <tbody>
                        @if (Model != null@*&& Model.Any()*@)
                            {

                            var i = 1;
                            foreach (var item in Model)
                            {
                            <tr>
                                <td>@i</td>
                                <td>@item.UserName</td>
                                <td>@item.Roles.Count</td>
                                <td>@item.Email</td>
                                <td>
                                    <a href="#" @*data-user="@item.UserName"*@ class="btn btn-sm btn-danger">Xóa</a>
                                </td>
                            </tr>
                            i++;
                                }
                            }
                    </tbody>
                </table>
            </div>
        </div>
    </section>

